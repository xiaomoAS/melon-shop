page {
  background: #F4F4F4;
}
.adres_detail_cont {
  margin: 20rpx auto;
  padding: 20rpx;
  background: #fff;
  border-radius: 10rpx;
  margin-bottom: 20rpx;
}
.adres_detail_cont .dl {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 25rpx 0;
  border-bottom: 1rpx solid #F1F1F1;
}
.adres_detail_cont .dl .dt {
  display: flex;
  align-items: center;
  font-size: 28rpx;
  font-weight: bold;
  color: #000;
}
.adres_detail_cont .dl .dt text {
  color: #FF0000;
  font-weight: bold;
  display: inline-block;
}
.adres_detail_cont .dl .dd input {
  width: 100%;
  height: 45rpx;
  text-align: right;
  font-size: 28rpx;
}
.adres_detail_cont .dl picker {
  width: 100%;
}
.adres_detail_cont .dl .uni-input {
  display: flex;
  align-items: center;
  justify-content: flex-end;
}
.adres_detail_cont .dl .uni-input .arr {
  width: 16rpx;
  margin-left: 10rpx;
}
.adres_detail_cont .deta_inf {
  width: 100%;
  height: 268rpx;
  padding: 10rpx;
  background: #F1F1F1;
  border-radius: 16rpx;
  font-size: 28rpx;
  color: #999;
  box-sizing: border-box;
}
.adres_default_seet {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx;
  background: #fff;
  border-radius: 10rpx;
}
.adres_default_seet .tle {
  font-size: 28rpx;
  color: #000000;
  font-weight: bold;
}
