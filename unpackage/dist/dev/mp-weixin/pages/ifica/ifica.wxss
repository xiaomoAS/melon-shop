.ifica_search_head {
  height: 130rpx;
  padding: 20rpx 20rpx 40rpx;
  background: linear-gradient(to left, #61C65E, #4B9F49);
}
.ifica_search_head .int_cont {
  position: relative;
}
.ifica_search_head .int_cont input {
  width: 100%;
  height: 70rpx;
  border-radius: 100rpx;
  padding-left: 70rpx;
  background: #fff;
  font-size: 24rpx;
  color: #333;
  box-sizing: border-box;
}
.ifica_search_head .int_cont input::-webkit-input-placeholder {
  color: #999;
}
.ifica_search_head .int_cont input::placeholder {
  color: #999;
}
.ifica_search_head .int_cont .i {
  position: absolute;
  left: 20rpx;
  top: 50%;
  transform: translateY(-50%);
  width: 24rpx;
}
.ifiac_filter_cont {
  border-top-right-radius: 20rpx;
  background: linear-gradient(#ceedcd, #fff 10%);
  height: calc(100vh - 210rpx);
  padding-left: 130rpx;
  position: relative;
  z-index: 2;
  margin-top: -20rpx;
}
.ifiac_filter_cont .l_filter_nav {
  width: 130rpx;
  position: absolute;
  top: 0;
  left: 0;
  height: calc(100vh - 110rpx);
  overflow-y: scroll;
  background: #F4F4F4;
  padding: 20rpx 0 0;
}
.ifiac_filter_cont .l_filter_nav .li {
  position: relative;
  overflow: hidden;
  padding: 14rpx 8rpx;
  text-align: center;
  border-top-left-radius: 10rpx;
  border-bottom-left-radius: 10rpx;
  margin-bottom: 10rpx;
  font-size: 24rpx;
  color: #999999;
}
.ifiac_filter_cont .l_filter_nav .li::after {
  content: "";
  display: block;
  width: 6rpx;
  height: 26rpx;
  border-top-right-radius: 5rpx;
  border-bottom-right-radius: 5rpx;
  position: absolute;
  left: -6rpx;
  top: 50%;
  transform: translateY(-50%);
  transition: all 0.6s;
  background: #61C55E;
}
.ifiac_filter_cont .l_filter_nav .li.active {
  background: #fff;
  color: #60C45D;
  font-weight: bold;
}
.ifiac_filter_cont .l_filter_nav .li.active::after {
  left: 0;
}
.ifiac_filter_cont .l_filter_nav .li:hover {
  background: #fff;
  color: #60C45D;
  font-weight: bold;
}
.ifiac_filter_cont .l_filter_nav .li:hover::after {
  left: 0;
}
.ifiac_filter_cont .main_cont {
  height: calc(100vh - 110rpx);
  overflow-y: scroll;
}
.test_report_dc {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
  background: linear-gradient(#C7FFCF, #FFFFFF 30rpx);
  z-index: 12;
  padding: 20rpx;
}
.test_report_dc .title_head {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 0 20rpx 0;
  margin-bottom: 20rpx;
  position: relative;
}
.test_report_dc .title_head::after {
  content: "";
  display: block;
  width: 100%;
  height: 1rpx;
  background: #efefef;
  position: absolute;
  bottom: 0;
  left: 0;
}
.test_report_dc .title_head .tle {
  display: flex;
  align-items: center;
  font-size: 30rpx;
  color: #000000;
}
.test_report_dc .title_head .tle .i {
  width: 21rpx;
  margin-right: 10rpx;
}
.test_report_dc .title_head .arr {
  width: 14rpx;
}
.test_report_dc .report_img {
  width: 100%;
  max-height: 60vh;
  overflow: scroll;
}
.test_report_dc .report_img image {
  display: block;
  width: 100%;
}
.index_pro_contain {
  border-radius: 20rpx;
  margin: 0 auto;
  background: #fff;
  position: relative;
  padding: 20rpx;
}
.index_pro_contain .pro_head_bg {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
}
.index_pro_contain .title_head {
  display: flex;
  align-items: center;
  font-size: 24rpx;
  color: #60C45D;
  position: relative;
  z-index: 2;
  margin-bottom: 20rpx;
}
.index_pro_contain .title_head .logo {
  width: 44rpx;
  margin-right: 14rpx;
}
.index_pro_contain .title_head .dt {
  font-size: 30rpx;
  font-weight: bold;
  color: #60C45D;
  margin-right: 10rpx;
}
.index_pro_contain .title_head .txt {
  padding: 0 20rpx;
  font-size: 24rpx;
  color: #60C45D;
  border-left: 1rpx solid #60C45D;
}
.pro_list_cont {
  margin-bottom: 20rpx;
}
.pro_list_cont .pro_i {
  display: block;
  width: 100%;
  border-radius: 10rpx;
  overflow: hidden;
  margin-bottom: 20rpx;
  position: relative;
  background: #000;
}
.pro_list_cont .pro_i .i {
  display: block;
  width: 100%;
}
.pro_list_cont .pro_i .i_over {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 146rpx;
  display: none;
}
.pro_list_cont .name {
  font-size: 28rpx;
  color: #000000;
  margin-bottom: 20rpx;
}
.pro_list_cont .det_txt {
  font-size: 24rpx;
  color: #FF5F36;
  margin-bottom: 20rpx;
}
.pro_list_cont .bit_deta {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 20rpx;
}
.pro_list_cont .bit_deta .tips_dl {
  display: flex;
  align-items: center;
  width: 70%;
}
.pro_list_cont .bit_deta .tips_dl .dd {
  padding: 6rpx 12rpx;
  margin-right: 6rpx;
  border-radius: 5rpx;
  font-size: 20rpx;
}
.pro_list_cont .bit_deta .tips_dl .dd.bg_1 {
  background: rgba(255, 95, 54, 0.1);
  color: #FF5F36;
}
.pro_list_cont .bit_deta .tips_dl .dd.bg_2 {
  background: rgba(26, 95, 255, 0.1);
  color: #1A5FFF;
}
.pro_list_cont .bit_deta .tips_dl .dd.bg_3 {
  background: rgba(96, 196, 93, 0.1);
  color: #60C45D;
}
.pro_list_cont .bit_deta .ys_time {
  display: inline-flex;
  align-items: center;
  border-radius: 5rpx;
  overflow: hidden;
  background: rgba(255, 54, 54, 0.1);
}
.pro_list_cont .bit_deta .ys_time .dt {
  border-radius: 5rpx;
  font-size: 20rpx;
  color: #fff;
  letter-spacing: 0;
  padding: 8rpx 5rpx;
  background: #FF3636;
}
.pro_list_cont .bit_deta .ys_time .dd {
  padding: 8rpx 10rpx;
  font-size: 20rpx;
  color: #FF3636;
}
.pro_list_cont .bit_seet {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.pro_list_cont .bit_seet .price {
  display: inline-flex;
  align-items: flex-end;
  font-size: 24rpx;
  color: #FF5F36;
}
.pro_list_cont .bit_seet .price text {
  font-size: 40rpx;
  font-weight: bold;
  line-height: 40rpx;
}
.pro_list_cont .bit_seet .price .dd {
  font-size: 24rpx;
  color: #60C45D;
}
.pro_list_cont .bit_seet .btn_dl {
  display: flex;
  align-items: center;
}
.pro_list_cont .bit_seet .btn_dl .btn {
  padding: 10rpx 16rpx;
  border-radius: 100rpx;
  margin-left: 10rpx;
  font-size: 24rpx;
}
.pro_list_cont .bit_seet .btn_dl .btn.btn_ys_1 {
  background: rgba(255, 95, 54, 0.1);
  color: #FF5F36;
}
.pro_list_cont .bit_seet .btn_dl .btn.btn_ys_2 {
  background: #60C45D;
  color: #fff;
}
.pro_list_cont.null .pro_i .i {
  opacity: 0.5;
}
.pro_list_cont.null .pro_i .i_over {
  display: block;
}
.pro_list_cont.null .bit_cont {
  opacity: 0.5;
}
.pro_list_cont.null .bit_cont .bit_deta .ys_time {
  display: none;
}
